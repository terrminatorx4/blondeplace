{
  "name": "blondeplace-blog",
  "type": "module",
  "version": "1.0.0",
  "description": "Блог-сателлит для салона красоты BlondePlace.ru - экспертные статьи о красоте, уходе за волосами и трендах",
  "author": "BlondePlace Beauty Team",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "node --max-old-space-size=4096 --expose-gc ./node_modules/.bin/astro build",
    "build:safe": "node --max-old-space-size=3072 --expose-gc ./node_modules/.bin/astro build",
    "build:large": "node --max-old-space-size=6144 --expose-gc ./node_modules/.bin/astro build",
    "preview": "astro preview",
    "astro": "astro",
    "factory": "node factory.js",
    "blitzkrieg": "node blitzkrieg-factory.js",
    "fix-conflicts": "node fix-merge-conflicts.js",
    "fix-yaml": "node yaml-fixer.js",
    "quick-test": "node quick-test-netlify.js",
    "emergency-clean": "node emergency-cleanup.js",
    "restore-fix": "node restore-and-fix.js"
  },
  "dependencies": {
    "@astrojs/netlify": "^5.5.3",
    "@astrojs/sitemap": "^3.2.1",
    "@google/generative-ai": "^0.21.0",
    "astro": "^4.16.17"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "execa": "^9.5.1",
    "gray-matter": "^4.0.3",
    "node-fetch": "^2.6.7"
  },
  "keywords": [
    "beauty-blog",
    "hair-care",
    "salon",
    "blonde-hair",
    "beauty-tips",
    "hair-coloring",
    "astro-blog",
    "beauty-seo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/terrminatorx4/blondeplace.git"
  },
  "homepage": "https://blondeplace.netlify.app"
}
